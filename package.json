{
  "name": "fandomscraper",
  "version": "1.0.5",
  "type": "module",
  "description": "A package to scrap fandoms wikis characters page. Only scraps the characters info section and the list of all repertoried characters.",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.json && node ./bundle.cjs",
    "test": "npx jest --config=jest.config.cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dilaouid/FandomScraper.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "fandom",
    "wikia",
    "wiki",
    "scraper",
    "characters",
    "info",
    "list"
  ],
  "author": "dilaouid",
  "license": "GPLv3",
  "private": false,
  "bugs": {
    "url": "https://github.com/dilaouid/FandomScraper/issues"
  },
  "homepage": "https://github.com/dilaouid/FandomScraper#readme",
  "dependencies": {
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.19",
    "@types/jest": "^29.5.12",
    "@types/jsdom": "^21.1.7",
    "chai": "^4.5.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.2.5",
    "typescript": "^5.5.4"
  }
}
